/*
   01uberheader.html
*/

#spacer {
  margin-top: 4px;
}

#banner {
  width: 1024px;
  background: #FFA812;
  height: 30px;
}


* {
  padding: 0;
  margin-top: 0;
  margin-right: auto;
  margin-bottom: 0;
  margin-left: auto;
} 

body {
  background: #F4F7ED repeat-x;
  font-family: Verdana, Arial, Helvetica, sans-serif;
  font-size: 11px;
  line-height: 18px;
  color: #333333;;
}

img { border: none; }

#wrap {
  width: 1000px;
  margin-top: 0;
  margin-bottom: 0;
}

#header {
  height: 60px;
  background: #26478A url(images/header.jpg) no-repeat;
  margin-left: 100px;
}

#header h1 {
  font-size: 26px;
  font-weight: 100;
  letter-spacing: -3px;
  padding: 12px 0 5px 10px;
}
#header h1 a {
  color: #F4F7EDF;
  text-decoration: none;
}
#header h1 a:hover {
  color: #F4F7ED;
  text-decoration: none;
}
#header h2 {
  color: #F4F7ED;
  font-size: 15px;
  font-weight: 100;
  padding: 0 0 0 11px;
  letter-spacing: -1px;
  line-height: 12px;
}

#intro {
  background: #CDDDAD url(images/intro.jpg) no-repeat;
  height: 120px;
  margin-left: auto;
  width: 700px;
  padding-top: 20px;
  padding-right: 30px;
  padding-bottom: 20px;
  padding-left: 30px;
}

#content {
  padding: 10px 20px;
}

.left {
  float: left;
  text-align: justify;
  margin-top: 0px;
}
.left h2 {
  color: #F4F7ED;
  font-size: 18px;
  letter-spacing: 0px;
  font-weight: 100;
  padding : 10px 0 15px 0;
}
.left ul {
  list-style-type: square;
  padding: 5px 10px 10px 20px;
}

.right {
  float: right;
  text-align: justify;
  margin-top: 0px;
}
.right h2 {
  color: #F4F7ED;
  font-size: 18px;
  letter-spacing: 0px;
  font-weight: 100;
  padding : 10px 0 15px 0;
}
.right ul {
  list-style-type: square;
  padding: 5px 10px 10px 20px;
}


#footer {
  text-align: center;
  color: #333;
  font-size: 11px;
  padding: 0 0 0px 0;
  margin-top: 0px;
}
#wrap .right .mdlinks {
  float: right;
  width: 130px;
  margin-left: 10px;
}
#wrap .left .articles {
  width: 700px;
  margin-right: 0px;
  float: none;
  margin-left: 150px;
}
#wrap .left .uplinks {
  width: 700px;
  margin-right: 0px;
  float: none;
  margin-left: 150px;
  margin-bottom: 20px;
}
#wrap .left .mdlinks {
  float: left;
  width: 140px;
  margin-right: 10px;
  text-align: left;
}


/*

Defining Hyperlink Colors

*/

a:link		{ color: #78AB46; text-decoration: none; }

a:visited	{ color: #78AB46; text-decoration: none; }

a:active	{ color: #FFC456; text-decoration: none; }    /* Same as "hover" for menus - doesn't work as intended */

a:hover		{ color: #FFA500; text-decoration: none; }



/*

Moved CSS from 01uberheader.html to here:

*/

#topcontainer {
    width: 100&#37;;
    text-align:justify;
}

.urbangreymenu{
    width: 160px; /*width of menu*/
	float: left;
	text-align: left;
	margin-top: 0px;
}

.urbangreymenu .headerbar{
font: bold 11px Verdana, Arial, Helvetica;
color: #FFFFFF;
background: #78AB46 no-repeat 8px 6px;
/* background-image: url('/images/arrowstop.gif') no-repeat 8px 6px; /*last 2 values 
are the x and y coordinates of bullet image*/ */
margin-bottom: 0; /*bottom spacing between header and rest of content*/
text-transform: uppercase;
padding: 5px 5px 5px 5px; /*31px is left indentation of header text*/
}

.urbangreymenu .headerbar a{
text-decoration: none;
color: #FFFFFF;
display: block;
}

.urbangreymenu ul{
list-style-type: none;
margin: 0;
padding: 0;
margin-bottom: 0; /*bottom spacing between each UL and rest of content*/
}

.urbangreymenu ul li{
padding-bottom: 2px; /*bottom spacing between menu items*/
}

.urbangreymenu ul li a{
font: normal 11px Verdana, Arial, Helvetica;
color: #333333;
background: #C9E0B3;
display: block;
padding: 5px 0;
line-height: 14px;
padding-left: 8px; /*link text is indented 8px*/
text-decoration: none;
}

.urbangreymenu ul li a:visited{
color: #333333;
}


/* Left Menu - link stays higlighted ("hover persists) while on page */

#l_menu { width:160px; font-family:Verdana, Arial, Helvetica, serif; font-size:11px; }
#l_menu ul { list-style: none; }
#l_menu ul li { }
#l_menu ul li a { display:block; text-decoration:none; color:#333333; background-color:#C9E0B3;
  padding-left:8px; cursor:pointer; }
#l_menu ul li a:hover { color:#FFFFFF; background-color:#FFC456; }                                             /* "mouseover", not dependant on scripts.js*/
#l_menu ul li a.aSelected { display:block; text-decoration:none; color:#FFFFFF; background-color:#FFA500; }   /* "click", dependant on scripts.js*/


/* Right Menu - same as above w/o "hover persists) */

#r_menu { width:160px; font-family:Verdana, Arial, Helvetica, serif; font-size:11px; }
#r_menu ul { list-style: none; }
#r_menu ul li { }
#r_menu ul li a { display:block; text-decoration:none; color:#333333; background-color:#C9E0B3;
  padding-left:8px; cursor:pointer; }
#r_menu ul li a:hover { color:#FFFFFF; background-color:#FFC456; }                                             /* "mouseover", not dependant on scripts.js*/